# API Keys for stock data providers
VITE_FINNHUB_KEY=your_finnhub_api_key_here
VITE_ALPHA_VANTAGE_KEY=your_alphavantage_api_key_here
# For Next.js exposure to browser use NEXT_PUBLIC_ prefix
NEXT_PUBLIC_FINNHUB_TOKEN=${VITE_FINNHUB_KEY}
NEXT_PUBLIC_ALPHA_VANTAGE_TOKEN=${VITE_ALPHA_VANTAGE_KEY}
NEXT_PUBLIC_TIINGO_TOKEN=your_tiingo_api_key_here
# Server-side Finnhub API key. Define only on the server and never expose to the browser
FINNHUB_API_KEY=

# Optional: Analytics (if used)
NEXT_PUBLIC_ANALYTICS_ID=

# Vercel specific settings (handled automatically in Vercel deployments)
VERCEL_URL=
VERCEL_ENV=production

# Application settings
# Optional: freeze all price lookups to a specific trading day (YYYY-MM-DD)
NEXT_PUBLIC_FREEZE_DATE=
NEXT_PUBLIC_APP_URL=https://your-domain.com
NODE_ENV=production
